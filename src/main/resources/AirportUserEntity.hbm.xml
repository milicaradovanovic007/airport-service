<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.milicaradovanovic.daon.entity.AirportUserEntity" table="airport_user" schema="mysql">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="email">
            <column name="email" sql-type="varchar(320)" length="320"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="isAdmin">
            <column name="is_admin" sql-type="tinyint(4)"/>
        </property>
    </class>
</hibernate-mapping>